{
    "name": "YouTube Auto PiP",
    "manifest_version": 2,
    "version": "1.0.0.0",
    "description":"YouTubeを再生中のタブのアクティブ状態に応じて自動でピクチャインピクチャを設定",
    "browser_action": {
      "default_title": "YouTube Auto PiP"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content_scripts.js"]
      }
    ]
}
